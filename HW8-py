def second_min_n_variables(elements):
    min_v = elements[0]
    for x in elements:
        if x < min_v:
            min_v = x

    second_min_v = elements[0]
    for x in elements:
        if x < second_min_v:
            if x > min_v:
                second_min_v = x 
    return second_min_v 

elements = [ 1, 0, 9, 4]
assert second_min_n_variables(elements) == 1